{
  "name": "catbee",
  "version": "1.0.0-rc1",
  "author": {
    "name": "Kirill Kaysarov",
    "email": "k.kaysarov@2gis.ru"
  },
  "description": "Catbee - High level framework based on best practices from Catberry, Baobab and Cerebral",
  "homepage": "https://github.com/markuplab/catbee",
  "keywords": [
    "catberry",
    "state",
    "immutable",
    "singals",
    "isomorphic",
    "morphdom",
    "tree"
  ],
  "main": "./dist/index",
  "browser": {
    "./dist/lib/Bootstrapper.js": "__BrowserBundle.js",
    "./dist/lib/helpers/hrTimeHelper.js": "./dist/browser/helpers/hrTimeHelper.js",
    "./dist/lib/loaders/ComponentLoader.js": "./dist/browser/loaders/ComponentLoader.js",
    "./dist/lib/loaders/SignalLoader.js": "./dist/browser/loaders/SignalLoader.js",
    "./dist/lib/loaders/WatcherLoader.js": "./dist/browser/loaders/WatcherLoader.js",
    "./dist/lib/providers/ModuleApiProvider.js": "./dist/browser/providers/ModuleApiProvider.js",
    "./dist/lib/Catberry.js": "./dist/browser/Catberry.js",
    "./dist/lib/DocumentRenderer.js": "./dist/browser/DocumentRenderer.js",
    "./dist/lib/RequestRouter.js": "./dist/browser/RequestRouter.js"
  },
  "dependencies": {
    "appstate": "^1.0.0",
    "babel-runtime": "^5.6.14",
    "baobab": "^2.0.0",
    "browser-process-hrtime": "^0.1.2",
    "browserify": "^12.0.1",
    "catberry-es6": "^1.0.1",
    "catberry-locator": "^1.1.7",
    "catberry-uhr": "^4.2.2",
    "catberry-uri": "^2.1.4",
    "chokidar": "^1.2.0",
    "entities": "^1.1.1",
    "glob": "^5.0.15",
    "mkdirp": "^0.5.1",
    "morphdom": "^1.0.3",
    "pretty-hrtime": "^1.0.1",
    "uglify-js": "^2.5.0",
    "watchify": "^3.6.0",
    "winston": "^2.1.0",
    "winston-console-formatter": "^0.1.0",
    "winston-graylog2": "^0.2.3"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-eslint": "^4.0.5",
    "babel-lab": "^1.0.0",
    "eslint": "^1.1.0",
    "lab": "^7.2.0"
  },
  "scripts": {
    "lint": "node ./node_modules/.bin/eslint lib/* browser/*",
    "test": "node ./node_modules/.bin/lab -T node_modules/babel-lab -l",
    "coverage": "node ./node_modules/.bin/lab -T node_modules/babel-lab -r html -o coverage.html -S -l",
    "build": "node ./node_modules/.bin/babel lib -d dist/lib && node ./node_modules/.bin/babel browser -d dist/browser",
    "prepublish": "npm run build"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/markuplab/catbee/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:markuplab/catbee.git"
  }
}
