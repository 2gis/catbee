{
  "name": "catbee",
  "version": "0.0.1",
  "author": {
    "name": "Kirill Kaysarov",
    "email": "k.kaysarov@2gis.ru"
  },
  "description": "Catbee - High level framework based on best practices from Catberry, Baobab and Cerebral",
  "homepage": "https://github.com/markuplab/catbee",
  "keywords": [
    "catberry",
    "state",
    "immutable",
    "singals",
    "isomorphic",
    "morphdom",
    "tree"
  ],
  "bugs": {
    "url": "https://github.com/markuplab/catbee/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:markuplab/catbee.git"
  },
  "main": "./index",
  "browser": {
    "./lib/Bootstrapper.js": "__BrowserBundle.js",
    "./lib/Catberry.js": "./browser/Catberry.js",
    "./lib/providers/ModuleApiProvider.js": "./browser/providers/ModuleApiProvider.js",
    "./lib/RequestRouter.js": "./browser/RequestRouter.js"
  },
  "dependencies": {
    "baobab": "^2.0.0",
    "catberry-locator": "^1.1.7",
    "catberry-uri": "^2.1.4",
    "chokidar": "^1.2.0",
    "entities": "^1.1.1",
    "glob": "^5.0.15",
    "promise": "^7.0.4"
  },
  "devDependencies": {
    "babel-lab": "^1.0.0",
    "babel-eslint": "^4.1.3",
    "eslint": "^1.6.0",
    "lab": "^6.1.0"
  },
  "scripts": {
    "lint": "node ./node_modules/.bin/eslint lib/* browser/*",
    "test": "node ./node_modules/.bin/lab -T node_modules/babel-lab -D",
    "coverage": "node ./node_modules/.bin/lab -T node_modules/babel-lab -r html -o coverage.html -S"
  }
}
